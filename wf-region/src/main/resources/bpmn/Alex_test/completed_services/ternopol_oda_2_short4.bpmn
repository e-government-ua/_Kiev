    </sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="mailtask3" targetRef="endevent2"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="mailtask2" targetRef="endevent2"></sequenceFlow>
  </process>
