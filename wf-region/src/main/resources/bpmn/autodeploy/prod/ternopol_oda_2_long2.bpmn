        <activiti:formProperty id="organID" name="До якого державного органу Ви звертаєтесь" type="enum" required="true">
          <activiti:value id="deptTernOda" name="Департамент соціального захисту населення Тернопільської ОДА"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="citizenType" name="До якої категорії громадян належите Ви або особа, яку ви представляєте" type="enum" required="true">
          <activiti:value id="Малозабезпечена непрацююча особа" name="Малозабезпечена непрацююча особа"></activiti:value>
          <activiti:value id="Інвалід" name="Інвалід"></activiti:value>
          <activiti:value id="Дитина-інвалід" name="Дитина-інвалід"></activiti:value>
          <activiti:value id="Пенсіонер" name="Пенсіонер"></activiti:value>
          <activiti:value id="Особа, яка брала участь в АТО" name="Особа, яка брала участь в АТО"></activiti:value>
          <activiti:value id="Особа, яка брала участь в акціях громадського протесту" name="Особа, яка брала участь в акціях громадського протесту"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="citizenTypeExtended" name="Вказажіть групу і причину інвалідності" type="string"></activiti:formProperty>
        <activiti:formProperty id="helpTo" name="Прошу надати матеріальну допомогу" type="enum" required="true">
          <activiti:value id="Мені" name="Мені"></activiti:value>
          <activiti:value id="На підопічного" name="На підопічного"></activiti:value>
          <activiti:value id="На дитину-інваліда" name="На дитину-інваліда"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="lastName_UA1" name="Прізвище підопічного/ дитини-інваліда" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="firstName_UA1" name="Ім'я підопічного/ дитини-інваліда" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="middleName_UA1" name="По батькові/ дитини-інваліда" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="helpeeAddress" name="Місце проживання (реєстрації) підопічного/ дитини-інваліда" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="helpeePassport" name="Дані паспорта/ свідоцтва про народження підопічного/ дитини-інваліда" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="helpeeINN" name="ІПН підопічного/ дитини-інваліда; якщо є" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="helpReason" name="Допомога потрібна у зв’язку з" type="enum" required="true">
          <activiti:value id="Тривалою хворобою" name="Тривалою хворобою"></activiti:value>
          <activiti:value id="Смертю близьких родичів (одного з подружжя, батьків, дітей)" name="Смертю близьких родичів (одного з подружжя, батьків, дітей)"></activiti:value>
          <activiti:value id="Стихійним лихом" name="Стихійним лихом"></activiti:value>
          <activiti:value id="Інші особливі обставини" name="Інші особливі обставини"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="transfer_type" name="Прошу в разі призначення допомоги кошти перераховувати; оберіть зручний для Вас спосіб перерахування коштів" type="enum" required="true">
          <activiti:value id="post" name="через національного оператора поштового зв'язку"></activiti:value>
          <activiti:value id="bank" name="на рахунок у банку"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="post_office" name="№ відділення поштового зв'язку (індекс)" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="bank_name" name="Назва банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="edrpou" name="ЄДРПОУ банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="bank_account" name="Номер рахунку (р/р)" type="string" default="  "></activiti:formProperty>
        <activiti:formProperty id="warning2" name="Зверніть увагу" type="label" default="Документи, що додаються:"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Копія паспорта заявника" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_inn" name="Копія ідентифікаційного номеру" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="maloz" name="Копія посвідчення отримувача державної соціальної допомоги" type="file"></activiti:formProperty>
        <activiti:formProperty id="inval" name="Копія висновку медико-соціальної експертної комісії про встановлення інвалідності" type="file"></activiti:formProperty>
        <activiti:formProperty id="pens" name="Копія пенсійного посвідчення" type="file"></activiti:formProperty>
