<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="smila_rda-1" name="Сміла РДА - Видача довідки з  державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім`я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail; На цей e-mail  Ви отримаєте результат обробки Вашого звернення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddress" name="Домашня адреса" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="rai" name="Оберіть район, в якому розташована земельна ділянка" type="enum" required="true">
          <activiti:value id="smil" name="Смілянський район"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="place" name="Місце знаходження земельної ділянки" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="area" name="Загальна площа ділянки у га" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="goal" name="Мета отримання довідки" type="enum" required="true">
          <activiti:value id="Для отримання земельної ділянки у власність" name="Для отримання земельної ділянки у власність"></activiti:value>
          <activiti:value id="Для оформлення договору оренди" name="Для оформлення договору оренди"></activiti:value>
          <activiti:value id="Для подачі в податкову для нарахування податку на землю" name="Для подачі в податкову для нарахування податку на землю"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="dilianka" name="Викопіювання земельної ділянки в масштабі 1:2000" type="file" default=" " required="true"></activiti:formProperty>
        <activiti:formProperty id="dovidka" name="Довідка з селищной ради; якщо ділянка в населенному пункті" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="text" name="Зверніть увагу" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Загрузка файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <sequenceFlow id="flow5" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow6" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow15" sourceRef="servicetask1" targetRef="servicetask5"></sequenceFlow>
    <serviceTask id="servicetask5" name="Лист-привітання громадянина" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Видача довідки з  державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
			&lt;p&gt;
			Ваше звернення щодо видачі довідки з державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями успішно зареєстроване.&lt;br /&gt;
			Ви отримаєте відповідь протягом десяти робочих днів.
			&lt;/p&gt;
			&lt;p&gt; 
			За наявності питань Ви можете звернутися за телефоном (04733) 4-30-82.
			&lt;/p&gt;
			[pattern/mail/_common_signature_start.html]
			ЦНАП Смілянської РДА,&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow16" sourceRef="servicetask5" targetRef="servicetask6"></sequenceFlow>
    <serviceTask id="servicetask6" name="Повідомлення держслужбовця" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>cnap.smilarda@ukr.net</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Нове звернення на порталі igov.org.ua на тему: Видача довідки з  державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями. Заявник ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}</activiti:expression>
        </activiti:field>
        <activiti:field name="bcc">
          <activiti:string><![CDATA[zhigan.roman@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_employee_notify.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow17" sourceRef="servicetask6" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка даних" activiti:candidateGroups="cherk_${rai}_1">
      <extensionElements>
        <activiti:formProperty id="clFIO" name="Прізвище, ім`я, по-батькові громадянина" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddress" name="Адреса громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Телефон громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="e-mail громадянина" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clArea" name="Місцезнаходження і площа ділянки" type="string" default="${place}, площа ${area} га" writable="false"></activiti:formProperty>
        <activiti:formProperty id="goal" name="Мета отримання довідки" type="enum" writable="false">
          <activiti:value id="Для отримання земельної ділянки у власність" name="Для отримання земельної ділянки у власність"></activiti:value>
          <activiti:value id="Для оформлення договору оренди" name="Для оформлення договору оренди"></activiti:value>
          <activiti:value id="Для подачі в податкову для нарахування податку на землю" name="Для подачі в податкову для нарахування податку на землю"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="proverka" name="Рішення про прийняття або відхилення заяви" type="enum">
          <activiti:value id="ok" name="Довідку виготовлено"></activiti:value>
          <activiti:value id="bad" name="Заяву відхилено"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment" name="Якщо громадянину відмовлено, вкажіть причину у цьому полі" type="textArea" default=" "></activiti:formProperty>
        <activiti:formProperty id="result" name="Додайте скановану копію довідки, якщо заяву опрацьовано" type="file"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${dilianka},${dovidka}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" name="Заявка прийнята і обробляється" sourceRef="exclusivegateway1" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${proverka=='ok'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask3" name="Лист з довідкою" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>"${email}"</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Видача довідки з  державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>
            [pattern/mail/_common_header.html]
			&lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
			&lt;p&gt;Замовлена Вами довідка з державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями готова.&lt;br /&gt;
			Ви можете завантажити її з додатку до цього листа, та отримати оригінал у регіональному ЦНАП за адресою:&lt;br /&gt;
			20700, м.Сміла, вул. Леніна,37
			&lt;/p&gt; 
			&lt;p&gt;Режим роботи центру:&lt;br&gt;
			понеділок-четвер з 8.00 до 17.15, обідня перерва з 13.00 до 14.00;&lt;br&gt; 
			п’ятниця з 8.00 до 16.00, обідня перерва з 13.00 до 14.00;&lt;br&gt;
			вихідний субота, неділя
			&lt;/p&gt;  
			&lt;p&gt;телефон (04733) 4-30-82
			&lt;/p&gt;
			[pattern/mail/_common_signature_start.html]
			ЦНАП Смілянської РДА,&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_footer.html]
		  </activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>${result}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow12" sourceRef="servicetask3" targetRef="endevent2"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
     <sequenceFlow id="flow3" name="Недостатньо інформації" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${proverka=='bad'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="servicetask4" targetRef="endevent2"></sequenceFlow>
    <serviceTask id="servicetask4" name="Відмова: Недостатньо даних" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Видача довідки з  державної статистичної звітності про наявність земель та розподіл їх за власниками земель, землекористувачами, угіддями]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>
          	[pattern/mail/_common_header.html]
		    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
			&lt;p&gt;На жаль, для виконання вашого запиту недостатньо інформації. 
			&lt;/p&gt;
			&lt;p&gt;
			${comment}
			&lt;/p&gt;
			За необхідності Ви можете подати ще одну заяву на порталі &lt;a href="https://igov.org.ua/" target="_blank"&gt;igov.org.ua&lt;/a&gt;&lt;/p&gt;
			[pattern/mail/_common_signature_start.html]
			ЦНАП Смілянської РДА.&lt;/p&gt;
			[pattern/mail/_common_footer.html]
		 </activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>  
  </process>
