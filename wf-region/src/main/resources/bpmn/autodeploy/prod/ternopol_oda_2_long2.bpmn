  <process id="ternopol_2_long2" name="Тернопіль ОДА - Надання одноразової матеріальної допомоги" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище заявника" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я заявника" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові заявника" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані заявника" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="Реєстраційний номер облікової картки платника податків; Одержувача допомоги АБО його законного представника" type="string"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+38" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail для зворотнього зв'язку " type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddress" name="Місце проживання (реєстрації); Вкажіть район та назву населеного пункту, вулицю, номер будинку та квартири" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="whoIs" name="Заяву Ви подаєте як" type="enum" required="true">
          <activiti:value id="vlasn" name="отримувач домоги"></activiti:value>
          <activiti:value id="predst" name="законний представник отримувача допомоги"></activiti:value>
