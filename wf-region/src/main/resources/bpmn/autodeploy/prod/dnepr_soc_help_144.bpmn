<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="dnepr_soc_144" name="УПЗСН - Допомога на дітей, які перебувають під опікою чи піклуванням" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="long"></activiti:formProperty>
        <activiti:formProperty id="adress1" name="Адреса реєстрації" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="adress2" name="Адреса проживання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="choice_visit" name="Ваше звернення до Управління праці та соціального захисту населення за даной послугой" type="enum" required="true">
          <activiti:value id="visit_1" name="Первинне"></activiti:value>
          <activiti:value id="visit_2" name="Повторне"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="transfer_type" name="Прошу в разі призначення допомоги кошти перераховувати; оберіть зручний для Вас спосіб перерахування коштів" type="enum" required="true">
          <activiti:value id="post" name="через національного оператора поштового зв'язку"></activiti:value>
          <activiti:value id="bank" name="на рахунок у банку"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="post_office" name="№ відділення поштового зв'язку (індекс)" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="bank_name" name="Назва банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="edrpou" name="ЄДРПОУ банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="bank_account" name="Номер рахунку в банку" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="file1" name="Копія рішення органу опіки та піклування або суду про встановлення опіки чи піклування над дитиною-сиротою або дитиною, позбавленою батьківського піклування" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="file2" name="Копія свідоцтва про народження дитини" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="file3" name="Довідка з місця проживання заявника про склад сім'ї;(для дітей, над якими встановлено опіку чи піклування і є вихованцями дитячих навчальних закладів, що фінансуються не за рахунок бюджетних  коштів, - видана органом реєстрації довідка, у якій зазначаються статус дитячогонавчального закладу і за які кошти він фінансується, а також підтверджується факт перебування у ньому дітей)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="file4" name="Довідки про місячні розміри пенсії, аліментів, стипендії, державної допомоги, що одержує на дитину опікун чи піклувальник" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="file5" name="Довідка, видана органом праці та соціального захисту населення за місцем реєстрації опікуна чи піклувальника про те, що він не перебуває на обліку як одержувач допомоги на дітей, над якими встановлено опіку чи піклування, і не одержує зазначеної допомоги;(якщо опікун чи піклувальник проживає за місцем проживання дитини, яка перебуває під його опікою чи піклуванням)" type="file"></activiti:formProperty>
        <activiti:formProperty id="rName" name="номер р-на соцзащиты" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="area" name="название р-на соцзащиты" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="group" name="група" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="markers1" name="Вибір отримання коштів" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;post_office&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;transfer_type&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'post'&quot;},&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;bank_name&quot;,&quot;mfo&quot;,&quot;edrpou&quot;,&quot;bank_account&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;transfer_type&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'bank'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers2" name="Звернення (первинне, вторинне)" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;file1&quot;,&quot;file2&quot;,&quot;file3&quot;,&quot;file4&quot;,&quot;file5&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;choice_visit&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'visit_1'&quot;},&quot;ShowFieldsOnCondition_4&quot;:{&quot;aField_ID&quot;:[&quot;file4&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;choice_visit&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'visit_2'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="notice" name="Зверніть увагу" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <userTask id="usertask1" name="Обробка Управлінням (1 етап)" activiti:candidateGroups="${group}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adress1" name="Адреса реєстрації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adress2" name="Адреса проживання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-mail для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="transfer_type" name="В разі призначення допомоги кошти перераховувати" type="enum" writable="false">
          <activiti:value id="post" name="через національного оператора поштового зв'язку"></activiti:value>
          <activiti:value id="bank" name="на рахунок у банку"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="post_office" name="№ відділення поштового зв'язку (індекс)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bank_name" name="Назва банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="edrpou" name="ЄДРПОУ банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bank_account" name="Номер рахунку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="decision_step1" name="Рішення етап 1" type="enum" required="true">
          <activiti:value id="attr1_ok" name="Заповнені дані вірні, інформація направлена на розгляд, очікується рішення"></activiti:value>
          <activiti:value id="attr2_bad" name="Дані заповнені невірно, відхилити звернення (обов'язково впишіть коментар нижче)"></activiti:value>
          <activiti:value id="attr5_bad" name="Дані надані не в повному обсязі - направити громадянину повідомлення (обов'язково впишіть коментар нижче)"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decision_comment" name="Якщо ви відмовляєте замовнику, обов'язково вкажіть підставу у цьому полі" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sBody" name="[pattern/print/dnepr_soc_help_144_print.html]" type="invisible" default="Форма заяви гомадянина" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${file1},${file2},${file3},${file4},${file5}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="servicetask4" targetRef="usertask1"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow4" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow6" name="decision_step1=='attr2_bad'" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_step1=='attr2_bad'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow7" sourceRef="servicetask5" targetRef="endevent2"></sequenceFlow>
    <userTask id="usertask2" name="Обробка Управлінням (2 етап)" activiti:candidateGroups="${group}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adress1" name="Адреса реєстрації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="adress2" name="Адреса проживання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-mail для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="transfer_type" name="В разі призначення допомоги кошти перераховувати" type="enum" writable="false">
          <activiti:value id="post" name="через національного оператора поштового зв'язку"></activiti:value>
          <activiti:value id="bank" name="на рахунок у банку"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="post_office" name="№ відділення поштового зв'язку (індекс)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bank_name" name="Назва банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="edrpou" name="ЄДРПОУ банку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bank_account" name="Номер рахунку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="decision_step1" name="Рішення етап 1" type="enum" writable="false">
          <activiti:value id="attr1_ok" name="Заповнені дані вірні, інформація направлена на розгляд, очікується рішення"></activiti:value>
          <activiti:value id="attr2_bad" name="Дані заповнені невірно, відхилити звернення (обов'язково впишіть коментар нижче)"></activiti:value>
          <activiti:value id="attr5_bad" name="Дані надані не в повному обсязі - направити громадянину повідомлення (обов'язково впишіть коментар нижче)"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decision_step2" name="Рішення етап 2" type="enum" required="true">
          <activiti:value id="attr3_ok" name="Рішення отримане, допомогу призначено"></activiti:value>
          <activiti:value id="attr4_bad" name="Рішення отримане, в допомозі відмовлено"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="document" name="Сканована копія рішення" type="file" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow8" name="decision_step1=='attr1_ok'" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_step1=='attr1_ok'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <serviceTask id="servicetask2" name="E-mail про негативне рішення" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Допомога на дітей, які перебувають під опікою чи піклуванням]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
На жаль, Вам відмовлено у призначенні допомоги на дітей, які перебувають під опікою чи піклуванням.&lt;br /&gt; 
&lt;p&gt;Відповідь від управління праці та соціального захисту населення знаходиться у додатку до листа.&lt;/p&gt;
&lt;br /&gt;
[pattern_dictonary:dnepr_soc_help.csv:value{[rName]}:5]
&lt;br /&gt;
[pattern/mail/_common_signature_start.html]
[pattern_dictonary:dnepr_soc_help.csv:value{[rName]}:2],&lt;br /&gt;
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>${document}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="E-mail про позитивне рішення" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Допомога на дітей, які перебувають під опікою чи піклуванням]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
Ви отримали схвалення щодо свого звернення про призначення допомоги на дітей, які перебувають під опікою чи піклуванням.&lt;br /&gt;
&lt;p&gt;Відповідь від управління праці та соціального захисту населення знаходиться у додатку до листа.&lt;/p&gt;
&lt;br /&gt;
[pattern_dictonary:dnepr_soc_help.csv:value{[rName]}:5]
&lt;br /&gt;
[pattern/mail/_common_signature_start.html]
[pattern_dictonary:dnepr_soc_help.csv:value{[rName]}:2],&lt;br /&gt;
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_feedback.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>${document}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow10" name="decision_step2=='attr4_bad'" sourceRef="exclusivegateway2" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_step2=='attr4_bad'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="decision_step2=='attr3_ok'" sourceRef="exclusivegateway2" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_step2=='attr3_ok'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow12" sourceRef="servicetask3" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="servicetask2" targetRef="endevent2"></sequenceFlow>
    <serviceTask id="servicetask4" name="Повідомлення держслужбовця про новий запит" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:string><![CDATA[msvidran@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення.  Допомога на дітей, які перебувають під опікою чи піклуванням]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) колего!&lt;/h3&gt;
                                    &lt;p&gt;
У Вас в черзі на обробку зареєстровано нове звернення щодо призначення допомоги на дітей, які перебувають під опікою чи піклуванням.&lt;br /&gt;
&lt;br /&gt;
Заявник: ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}&lt;br /&gt;
Опрацюйте це звернення на &lt;a href="https://region.igov.org.ua" target="_blank"&gt;порталі&lt;/a&gt;
                                    &lt;/p&gt;
                                    &lt;hr style="width: 100%; height: 0; margin: 30px 0 15px 0; background: #dddddd; border: 0; border-bottom: 1px solid #dddddd;" /&gt;
                                    &lt;p&gt;
                                    З повагою,&lt;br /&gt;
					  				Команда Порталу державних послуг &lt;a href="https://igov.org.ua/" target="_blank"&gt;iGov&lt;/a&gt;
                                        &lt;/p&gt;
                                    &lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/table&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--content end--&gt;
