    &lt;tr&gt;
        &lt;td align="center"&gt;
            &lt;table width="800" cellpadding="0" cellspacing="0" border="0" style="border-collapse: collapse;"&gt;
                &lt;!--header line statrs--&gt;
                &lt;tr&gt;
                    &lt;td bgcolor="#5CA3CB" style="padding: 35px 120px; color: white;"&gt;&lt;b style="font-size: 24px;"&gt;iGov.&lt;span&gt;org.&lt;span&gt;ua&lt;/span&gt;&lt;/span&gt;&lt;/b&gt;&lt;br /&gt;портал державних послуг&lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--header line end--&gt;
                &lt;!--content statrs--&gt;
                &lt;tr&gt;
                    &lt;td align="center"&gt;
                        &lt;table width="800" cellpadding="0" cellspacing="0" border="0" style="border-collapse: collapse;"&gt;
                            &lt;tr&gt;
                                &lt;td style="padding: 35px 120px;"&gt;
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
                                        Ваш запит щодо призначення тимчасової державної допомоги дітям, батьки яких відхиляються від сплати аліментів, не мають можливості утримувати дитину або місце проживання їх невідоме було завантажено.&lt;/p&gt;
                            
                                    &lt;hr style="width: 100%; height: 0; margin: 30px 0 15px 0; background: #dddddd; border: 0; border-bottom: 1px solid #dddddd;" /&gt;
                                    &lt;p&gt;
                                        З повагою,&lt;br /&gt;
                                        Укрпошта,&lt;br /&gt;
                                        Команда Порталу державних послуг &lt;a href="#current-page"&gt;iGov.org.ua&lt;/a&gt;.
                                    &lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                           
                          

                        &lt;/table&gt;
                     
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--content end--&gt;
            &lt;/table&gt;
        &lt;/td&gt;
    &lt;/tr&gt;
    &lt;/tbody&gt;
&lt;/table&gt;</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="mailtask1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка Укрпоштою" activiti:candidateGroups="dnepr_post_111">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddress" name="Місце реєстрації" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="place_of_liv" name="Місце проживання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="e-mail замовника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="decision" name="Рішення про надання послуги" type="enum" required="true">
          <activiti:value id="attr4_finish" name="Зауважень немає. Підтвердити відправлення роздрукованих документів до соціальної служби"></activiti:value>
          <activiti:value id="attr2_doc_bad" name="Відмовити в наданні послуги"></activiti:value>
          <activiti:value id="attr1_doc_ok" name="Документи вірні. Відправити листа для оплати."></activiti:value>
          <activiti:value id="attr3_money_no" name="Оплата не надійшла"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment" name="Якщо ви відмовляєте замовнику у наданні послуги, обов'язково вкажіть підставу" type="textArea"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="mailtask1" targetRef="usertask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
