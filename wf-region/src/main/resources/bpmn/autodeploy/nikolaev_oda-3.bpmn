    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="mailtask2" targetRef="endevent2"></sequenceFlow>
    <serviceTask id="mailtask2" name="Відправлення листа громадянину" activiti:type="mail">
