      var no = ['14', '15', '16', '17', '18', '19', '20', '21']
      var region = [', '', '', 'Житомир', 'Кременчук', '', 'Вишгородський район', 'Червоноград']
      var Regservice = [', ' ', ' ', ' ', ' ', ' ', ' ', ' ', 'Вишгородський РВ ДМС України в Київській області', 'Червоноградський МС ДМС України у Львівській області'] 
	  var So = ['dndzT', 'ternR', 'od_komint', 'vin_teplic', 'zhitomT', 'pol_krem', 'kharkT', 'kiev_vish', 'lviv_chervonogradT']
for (var i = 0; i&lt;697; i++){
var per = So[i]
if(execution.getVariable('sID_Public_SubjectOrganJoin') == per){
var per1 = post[i]
var per2 = no[i]
var per3 = region[i]
var per4 = Regservice[i]
break}}
execution.setVariable('postV', per1)
execution.setVariable('viddil', per2)
execution.setVariable('districtS', per3)
execution.setVariable('Registrationservice', per4)
	  </script>
