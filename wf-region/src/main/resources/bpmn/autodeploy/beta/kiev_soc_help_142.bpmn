        <activiti:formProperty id="markers13" name="Маркери про трудову книжку" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;doc_employment_history&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;employment_history&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'yes_2'&quot;},&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;doc_employment_history_no&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;employment_history&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'no_2'&quot;}}}"></activiti:formProperty>
