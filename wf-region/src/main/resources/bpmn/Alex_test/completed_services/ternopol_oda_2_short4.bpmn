        <activiti:formProperty id="bankId_scan_inn" name="Скан-копія ІПН" type="file"></activiti:formProperty>
        <activiti:formProperty id="helpeeID" name="Посвідчення пенсіонера/отримувача державної соціальної допомоги АБО висновок МедЕкспертизи" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="helpeeIncome" name="Акт обстеження матеріально-побутових умов проживання" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="helpeeSurname" name="Прізвище отримувача допомоги; Заповніть якщо Ви законний представник" type="string"></activiti:formProperty>
        <activiti:formProperty id="helpeeName" name="Його ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="helpeeMiddleName" name="Його по-батькове" type="string"></activiti:formProperty>
        <activiti:formProperty id="helpeePassport" name="Паспортні дані/свідоцтва народження отримувача" type="string"></activiti:formProperty>
        <activiti:formProperty id="helpeePassportScan" name="Скан-копія паспорта/свідоцтва народження отримувача" type="file"></activiti:formProperty>
