        <activiti:formProperty id="helpeePassport" name="Паспортні дані отримувача" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="helpeePassportScan" name="Скан-копія паспорту отримувача" type="file" writable="false"></activiti:formProperty>
