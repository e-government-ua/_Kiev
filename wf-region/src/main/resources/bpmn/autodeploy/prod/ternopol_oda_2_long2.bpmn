        <activiti:formProperty id="chInval" name="Копія медичного висновку про дитину-інваліда віком до 18 років" type="file"></activiti:formProperty>
        <activiti:formProperty id="repProof" name="Копія документу, що засвідчує повноваження законного представника" type="file"></activiti:formProperty>
        <activiti:formProperty id="helpeePassportScan" name="Копія паспорта підопічного / свідоцтва про народження підопічного; Сторінки 1, 2 і 11" type="file"></activiti:formProperty>
        <activiti:formProperty id="childScan" name="Копія свідоцтва про народження дитини-інваліда" type="file"></activiti:formProperty>
        <activiti:formProperty id="scanINN" name="Копія ідентифікаційного номеру підопічного/ дитини-інваліда; Якщо є" type="file"></activiti:formProperty>
        <activiti:formProperty id="scanDeath" name="Копія свідоцтва про смерть" type="file"></activiti:formProperty>
        <activiti:formProperty id="scanMed" name="Копія висновку з медичної установи" type="file"></activiti:formProperty>
        <activiti:formProperty id="scanSum" name="Копія документу, що підтверджує суму завданих збитків" type="file"></activiti:formProperty>
        <activiti:formProperty id="scanOth" name="Копія документу, що підтверджує скрутний матеріальний стан" type="file"></activiti:formProperty>
        <activiti:formProperty id="warning" name="Зверніть увагу" type="label" default="Подаючи звернення, Ви підтверджуєте достовірність усіх зазначених у зверненні даних і надаєте свою згоду на обробку Ваших персональних даних"></activiti:formProperty>
        <activiti:formProperty id="marker1" name="документ представника" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;repProof&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;whoIs&quot;},&quot;sCondition&quot;:&quot;[sService]=='predst'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker2" name="дані підопічної особи" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;lastName_UA1&quot;,&quot;firstName_UA1&quot;,&quot;middleName_UA1&quot;,&quot;helpeeAddress&quot;,&quot;helpeePassport&quot;,&quot;helpeeINN&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpTo&quot;},&quot;sCondition&quot;:&quot;[sService]=='На підопічного' || [sService]=='На дитину-інваліда'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker3" name="дані для інвалідів" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;citizenTypeExtended&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;citizenType&quot;},&quot;sCondition&quot;:&quot;[sService]=='Інвалід' || [sService]=='Дитина-інвалід'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker4" name="куди відправити гроші" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_4&quot;:{&quot;aField_ID&quot;:[&quot;post_office&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;transfer_type&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'post'&quot;},&quot;ShowFieldsOnCondition_5&quot;:{&quot;aField_ID&quot;:[&quot;bank_name&quot;,&quot;mfo&quot;,&quot;edrpou&quot;,&quot;bank_account&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;transfer_type&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'bank'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker5" name="файл для підопічних" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_6&quot;:{&quot;aField_ID&quot;:[&quot;helpeePassportScan&quot;,&quot;scanINN&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpTo&quot;},&quot;sCondition&quot;:&quot;[sService]=='На підопічного'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker6" name="файл для дитини-інваліда" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_7&quot;:{&quot;aField_ID&quot;:[&quot;childScan&quot;,&quot;chInval&quot;,&quot;scanINN&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpTo&quot;},&quot;sCondition&quot;:&quot;[sService]=='На дитину-інваліда'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker7" name="файл при хворобі" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_8&quot;:{&quot;aField_ID&quot;:[&quot;scanDeath&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpReason&quot;},&quot;sCondition&quot;:&quot;[sService]=='Смертю близьких родичів (одного з подружжя, батьків, дітей)'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker8" name="файл при хворобі" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_9&quot;:{&quot;aField_ID&quot;:[&quot;scanMed&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpReason&quot;},&quot;sCondition&quot;:&quot;[sService]=='Тривалою хворобою'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker9" name="файл при стихійному лихові" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_10&quot;:{&quot;aField_ID&quot;:[&quot;scanSum&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpReason&quot;},&quot;sCondition&quot;:&quot;[sService]=='Стихійним лихом'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker10" name="файл для малозабезпечених" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_11&quot;:{&quot;aField_ID&quot;:[&quot;maloz&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;citizenType&quot;},&quot;sCondition&quot;:&quot;[sService]=='Малозабезпечена непрацююча особа'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker11" name="файл при пенсіонерів" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_12&quot;:{&quot;aField_ID&quot;:[&quot;pens&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;citizenType&quot;},&quot;sCondition&quot;:&quot;[sService]=='Пенсіонер'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker12" name="файл для АТО і демонстрантів" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_13&quot;:{&quot;aField_ID&quot;:[&quot;atoProtest&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;citizenType&quot;},&quot;sCondition&quot;:&quot;[sService]=='Особа, яка брала участь в АТО' || [sService]=='Особа, яка брала участь в акціях громадського протесту'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker13" name="файл для інваліда" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_14&quot;:{&quot;aField_ID&quot;:[&quot;inval&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;citizenType&quot;},&quot;sCondition&quot;:&quot;[sService]=='Інвалід'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker14" name="файл при інших обставинах" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_15&quot;:{&quot;aField_ID&quot;:[&quot;scanOth&quot;],&quot;asID_Field&quot;:{&quot;sService&quot;:&quot;helpReason&quot;},&quot;sCondition&quot;:&quot;[sService]=='Інші особливі обставини'&quot;}}}"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Підтягування документів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow2" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="servicetask1" targetRef="mailtask1"></sequenceFlow>
    <serviceTask id="mailtask1" name="Повідомлення про успішне замовлення" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Надання одноразової матеріальної допомоги громадянам]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression>&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head lang="ru"&gt;
    &lt;meta http-equiv="Content-Type" content="text/html; charset=UTF-8" /&gt;
    &lt;style type="text/css"&gt;
        body {margin: 0; padding: 0;}
        #mailer-body {font: 400 18px/20px 'Calibri';}
        #mailer-body div,
        #mailer-body p,
        #mailer-body span,
        #mailer-body strong,
        #mailer-body b,
        #mailer-body em,
        #mailer-body i,
        #mailer-body a,
        #mailer-body li,
        #mailer-body td {-webkit-text-size-adjust: none;}
        #mailer-body a {color: #2F70AF; text-decoration: none;}
        #mailer-body a:hover {color: #2e5e9c; text-decoration: underline;}
        #mailer-body label {height: 20px; line-height: 20px; position: relative; cursor: pointer; display: inline-block;}
        #mailer-body p &gt; label {width: 40px;}
        #mailer-body label input[type='radio'] {width: 16px; height: 16px; top: -1px; float: left; position: relative; opacity: 0;}
        #mailer-body label input[type='radio'] + span {position: absolute; margin: 0 15px 0 0; font-weight: 700; font-size: 16px;}
        #mailer-body label input[type='radio'] + span:before {content: " "; width: 22px; height: 20px; background: url(http://shots.jotform.com/elton/radio_flat.png) left -1px no-repeat; left: -25px; display: block; position: absolute;}
        #mailer-body label input[type='radio']:checked + span:before {background: url(http://shots.jotform.com/elton/radio_flat.png) left -22px no-repeat;}
        #mailer-body textarea {width: 548px; padding: 5px; min-height: 65px; height: 65px; max-height: 100px; margin: 10px 0 0 0; border-radius: 3px; border: 1px solid #dddddd; resize: vertical;}
        #mailer-body textarea:focus {outline: 0;}
        #mailer-body button {height: 30px; padding: 0 25px; border: 0; border-radius: 3px; line-height: 30px; font-size: 14px; font-weight: 600; background: #2F70AF; cursor: pointer; color: white; display: inline-block;}
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
&lt;table width="100%" cellspacing="0" cellpadding="0" border="0" style="border-collapse: collapse;" id="mailer-body"&gt;
    &lt;tbody&gt;
    &lt;tr&gt;
        &lt;td align="center"&gt;
            &lt;table width="800" cellpadding="0" cellspacing="0" border="0" style="border-collapse: collapse;"&gt;
                &lt;!--header line statrs--&gt;
                &lt;tr&gt;
                    &lt;td bgcolor="#5CA3CB" style="padding: 35px 120px; color: white;"&gt;&lt;b style="font-size: 24px;"&gt;iGov.org.ua&lt;/b&gt;&lt;br /&gt;портал державних послуг&lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--header line end--&gt;
                &lt;!--content statrs--&gt;
                &lt;tr&gt;
                    &lt;td align="center"&gt;
                        &lt;form action="[sURL_ServiceMessage]" method="post"&gt;
                        &lt;table width="800" cellpadding="0" cellspacing="0" border="0" style="border-collapse: collapse;"&gt;
                            &lt;tr&gt;
                                &lt;td style="padding: 35px 120px;"&gt;
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
										Ваш запит був успішно зареєстрований у нашій системі.&lt;br /&gt; 	
										Спеціалісти із державного органу повинні дати відповідь найближчим часом.
										&lt;/p&gt;
                                    &lt;hr style="width: 100%; height: 0; margin: 30px 0 15px 0; background: #dddddd; border: 0; border-bottom: 1px solid #dddddd;" /&gt;
                                    &lt;p&gt;
                                        З повагою,&lt;br /&gt;
                                        Команда Порталу державних послуг &lt;a href="#current-page"&gt;iGov.org.ua&lt;/a&gt;.
                                    &lt;/p&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;

                        &lt;/table&gt;
                        &lt;/form&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--content end--&gt;
            &lt;/table&gt;
        &lt;/td&gt;
    &lt;/tr&gt;
    &lt;/tbody&gt;
&lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="mailtask1" targetRef="mailtask4"></sequenceFlow>
    <serviceTask id="mailtask4" name="Повідомлення ЦНАП про нове звернення" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
