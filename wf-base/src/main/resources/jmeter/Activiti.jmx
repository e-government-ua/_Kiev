          <stringProp name="script">eval(&quot;var formResult = &quot; +vars.get(&quot;formDataResponse&quot;)+&quot;;&quot;);

var window = {};
eval(&quot;&quot; + vars.get(&quot;faker_js&quot;));
var faker = window.faker;

eval(&quot;&quot; + vars.get(&quot;date_format_js&quot;));

var formDefProps = formResult.formProperties;

var formProps = [];

for (var i = 0; i &lt; formDefProps.length; i++) {
  var fld = formDefProps[i];
  if (!fld.writable) continue;
  if (fld.required) {
    var rndVal;
    if (fld.enumValues &amp;&amp; fld.enumValues.length) {
      var rndIdx = Math.floor(Math.random() * fld.enumValues.length);
      rndVal = fld.enumValues[rndIdx].id;
    } else {
      if (fld.type==&apos;date&apos;) {
        rndVal =new Date().format(fld.datePattern);
      }
      if (fld.type==&apos;string&apos;) {
        if (fld.id == &apos;email&apos; || fld.id == &apos;mail&apos;) {
           rndVal  = vars.get(&quot;email&quot;);
        } else if (fld.id == &apos;firstName&apos;) {
           rndVal = faker.name.firstName();
        } else if (fld.id == &apos;lastName&apos;) {
           rndVal = faker.name.lastName();
        } else if (fld.id == &apos;phone&apos;) {
           rndVal = faker.phone.phoneNumber();
        }  else {
           rndVal = faker.lorem.words();
        }
      }
      if (fld.type==&apos;number&apos;) {
        rndVal = Math.floor(Math.random() * 2000);
      }
    }
   formProps.push({&quot;id&quot;:fld.id, &quot;value&quot;: rndVal});
  }
}

var formData = {
  &quot;taskId&quot; : vars.get(&quot;taskId&quot;),
  &quot;properties&quot; : formProps
};

var formDataPropertiesJson = &quot;[&quot;;


for (var j = 0; j &lt; formProps.length; j++) {
  if (j &gt; 0) formDataPropertiesJson += &quot;,&quot;;
  formDataPropertiesJson += &quot;{\&quot;id\&quot;:\&quot;&quot;+formProps[j].id+&quot;\&quot;,\&quot;value\&quot;:\&quot;&quot;+formProps[j].value+&quot;\&quot;}&quot;;
}
formDataPropertiesJson += &quot;]&quot;;

vars.put(&quot;formDataPropertiesJson&quot;, formDataPropertiesJson);

SampleResult.setResponseData( formDataPropertiesJson );
</stringProp>
