[pattern_dictonary:dnepr_reg_cnap.csv:value{[region]}:2]&lt;/p&gt;
&lt;p&gt;[pattern_dictonary:dnepr_reg_cnap.csv:value{[region]}:3]&lt;br /&gt;
[pattern_dictonary:dnepr_reg_cnap.csv:value{[region]}:4]&lt;br /&gt;
