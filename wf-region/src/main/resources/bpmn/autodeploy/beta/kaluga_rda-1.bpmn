        <activiti:formProperty id="bankIdAddress" name="Адрес" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="goal" name="Мета звернення" type="enum" writable="false">
          <activiti:value id="item1" name="визначення розміру земельного податку при  міні  або  даруванні  земельних  ділянок"></activiti:value>
