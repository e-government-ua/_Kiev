  <process id="dnepr_post_111" name="Укрпошта - Призначення тимчасової державної допомоги дітям, батьки яких ухиляються від сплати аліментів, не мають можливості утримувати дитину або місце проживання їх невідоме" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddress" name="Місце реєстрації (вкажіть область, місто, вулицю, будинок, квартиру)" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="place_of_liv" name="Місце проживання (вкажіть область, місто, вулицю, будинок, квартиру)" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="area" name="Оберіть районний орган соціального захисту населення, до якого Ви відноситесь" type="enum" required="true">
          <activiti:value id="sobes1" name="АНД, вул. Радістів, 8"></activiti:value>
          <activiti:value id="sobes2" name="Бабушкінський, вул. Героїв Сталінграда, 116а"></activiti:value>
          <activiti:value id="sobes3" name="Жовтневий, вул. Жуковського, 39/41"></activiti:value>
          <activiti:value id="sobes4" name="Індустріальний, вул. Косіора, 55"></activiti:value>
          <activiti:value id="sobes5" name="Кіровський, вул. Свердлова, 30/32"></activiti:value>
          <activiti:value id="sobes6" name="Красногвардійський, вул. Уральська, 5"></activiti:value>
          <activiti:value id="sobes7" name="Ленінський, вул. Трудова, 1а"></activiti:value>
          <activiti:value id="sobes8" name="Самарський, вул. 20-річчя Перемоги, 51"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+38" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="numobr" name="Ваше звернення до районного органу соціального захисту населення, до якого Ви відноситесь" type="enum" required="true">
          <activiti:value id="obr1" name="первинне"></activiti:value>
          <activiti:value id="obr2" name="вторинне"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="numossp" name="номер основної особової справи" type="string"></activiti:formProperty>
        <activiti:formProperty id="kuda_dengi" name="Оберіть" type="label" default="В разі призначення допомоги кошти готівкою перераховувати"></activiti:formProperty>
        <activiti:formProperty id="postnum" name="через національного оператора поштового зв’язку №" type="long"></activiti:formProperty>
        <activiti:formProperty id="kudadengi1" type="label" default="або"></activiti:formProperty>
        <activiti:formProperty id="bank_schet" name="на рахунок у банку №" type="long"></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО" type="long"></activiti:formProperty>
        <activiti:formProperty id="bankchoose" name="банк" type="string"></activiti:formProperty>
        <activiti:formProperty id="okpo" name="код" type="long"></activiti:formProperty>
        <activiti:formProperty id="rozgl" name="Якщо моя заява потребує розглядання комісією, прошу мою заяву " type="enum" required="true">
          <activiti:value id="roz1" name="розглянути"></activiti:value>
          <activiti:value id="roz2" name="не розглядати"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="text" name="Зверніть увагу" type="label" default="Я усвідомлюю, що наведені мною відомості про доходи та майно, що вплинули або могли вплинути на прийняте рішення щодо надання соціальної допомоги, компенсацій та пільг, будуть перевірені згідно з чинним законодавством України."></activiti:formProperty>
        <activiti:formProperty id="reg_reb" name="Завантажте довідку про реєстрацію місця проживання (перебування) дитини" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="scsvid" name="Завантажте копію свідоцтва про народження дитини" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="reg_oder" name="Завантажте довідку про реєстрацію місця проживання (перебування) одержувача" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="num_ark" name="Кількість сторінок у доданих документах" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="num_doc" name="Кількість доданих документів" type="long" required="true"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="mailtask1" name="Повідомлення громадянина про реєстрацію звернення" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Повідомлення про вдалу відправку документів]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression>&lt;style type="text/css"&gt;
        body {margin: 0; padding: 0;}
        #mailer-body {font: 400 18px/20px 'Calibri';}
