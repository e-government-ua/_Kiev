                                    &lt;button type="submit"&gt;Відправити&lt;/button&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                            &lt;/table&gt;
                        &lt;/form&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
                &lt;!--content end--&gt;
            &lt;/table&gt;
        &lt;/td&gt;
    &lt;/tr&gt;
