          <activiti:value id="dept" name="Департаменту економічного розвитку, інвестиційної діяльності та міжнародного співробітництва облдержадміністрації, щодо проблемних питань ведення бізнесу"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="problem" name="Суть звернення" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="resultWay" name="Спосіб отримання відповіді" type="enum" writable="false">
          <activiti:value id="fizic" name="Фізична копія"></activiti:value>
          <activiti:value id="digit" name="Електронна копія"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="docFile" name="Додати скан-копію офіційної відповіді" type="file" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
