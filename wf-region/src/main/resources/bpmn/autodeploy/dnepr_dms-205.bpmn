<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="dnepr_dms-205" name="Дніпропетровськ ДМС - Оформлення паспорта громадянина України по досягненні 16-річного віку" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По Батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="lable1" name="Зверніть увагу" type="label" default="Дані дитини, яка буде отримувати паспорт"></activiti:formProperty>
        <activiti:formProperty id="lastName_UA1" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="firstName_UA1" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="middleName_UA1" name="По Батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="kidsBirth" name="Дата народження; Формат - ДД.ММ.PPPP" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="kidsPOB" name="Місце народження; Країна, область, район і населений пункт" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="kidsSex" name="Стать" type="enum" required="true">
          <activiti:value id="man" name="Чоловіча"></activiti:value>
          <activiti:value id="woman" name="Жіноча"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="kids_liv" name="Місце проживання; Населений пункт, Вулиця, Будинок, Корпус, Квартира" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="lable2" name="Зверніть увагу" type="label" default="Дані батьків дитини, яка буде отримувати паспорт"></activiti:formProperty>
        <activiti:formProperty id="lastName_UA2" name="Прізвище" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="firstName_UA2" name="Ім'я" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="middleName_UA2" name="По Батькові" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="lastName_UA3" name="Прізвище" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="firstName_UA3" name="Ім'я" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="middleName_UA3" name="По Батькові" type="string" default=" "></activiti:formProperty>
        <activiti:formProperty id="district" name="Район м. Дніпропетровськ, у якому Ви плануєте отримувати паспорт" type="enum" required="true">
          <activiti:value id="bab" name="Бабушкінський"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="visitDay" name="Оберіть день та час, коли Вам буде зручно зайти до відділення ДМС?" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="svid" name="Свідоцтво про народження" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="pasp1" name="Паспорт одного з батьків; скан-копія всіх заповнених сторінок" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="pasp2" name="Паспорт другого з батьків; скан-копія всіх заповнених сторінок" type="file"></activiti:formProperty>
        <activiti:formProperty id="warning" name="Зверніть увагу" type="label" default="Подаючи звернення, Ви підтверджуєте достовірність усіх зазначених у зверненні даних і надаєте свою згоду на обробку Ваших персональних даних"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <sequenceFlow id="flow2" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Підтягування додаткових документів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow1" sourceRef="boundaryerror1" targetRef="endevent2"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="servicetask1" targetRef="servicetask3"></sequenceFlow>
    <serviceTask id="servicetask3" name="E-Mail підтвердження отримання запиту ДМС" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Оформлення паспорта громадянина України по досягненні 16-річного віку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
										Ваш запит був успішно зареєстрований у нашій системі. 
								    &lt;/p&gt;
									&lt;p&gt;
										Спеціалісти ДМС оброблять Вашу заявку протягом 1-3 днів та надішлють Вам email з підтвердженням та запрошенням відвідати ДМС у обраний час. 
									&lt;/p&gt;
                                    [pattern/mail/_common_signature_start.html]
									Державна Міграційна Служба Дніпропетровської Області,&lt;br /&gt;
									[pattern/mail/_common_signature_end.html]   
									[pattern/mail/_common_footer.html]
		  </activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="servicetask3" targetRef="servicetask4"></sequenceFlow>
    <serviceTask id="servicetask4" name="Повідомлення ДМС про нове звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[zhigan.roman@gmail.com]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення на тему: Оформлення паспорта громадянина України по досягненні 16-річного віку]]></activiti:string>
        </activiti:field>
        <activiti:field name="bcc">
          <activiti:string><![CDATA[zhigan.roman@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_employee_notify.html]
		  </activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow5" sourceRef="servicetask4" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка вхідних даних" activiti:candidateGroups="dnepr_dms-89_${district}">
      <extensionElements>
        <activiti:formProperty id="info" name="На цьому етапі:" type="label" default="Ви перевіряєте дані громадянина і вирішуєте чи всі вони вірні. Якщо все добре, призначаєте дату та час, коли громадянин має прийти до вас та принести документи для реєстрації заяви"></activiti:formProperty>
        <activiti:formProperty id="district" name="Район м. Дніпропетровськ, до якого належить місце реєстрації громадянина" type="enum" writable="false">
          <activiti:value id="bab" name="Бабушкінський"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="clFio1" name="ПІБ заявника" type="string"  default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="ПІБ отримувача паспорта" type="string" default="${lastName_UA1} ${firstName_UA1} ${middleName_UA1}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsBirth" name="Дата народження отримувача паспорта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsPOB" name="Місце народження отримувача паспорта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsSex" name="Стать" type="enum"  writable="false">
          <activiti:value id="man" name="Чоловіча"></activiti:value>
          <activiti:value id="woman" name="Жіноча"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="kids_liv" name="Місце проживання; Населений пункт, Вулиця, Будинок, Корпус, Квартира" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="lable2" type="label" default="Дані батьків дитини, яка буде отримувати паспорт"></activiti:formProperty>
        <activiti:formProperty id="parent1" name="ПІБ1" type="string" default="${lastName_UA2} ${firstName_UA2} ${middleName_UA2}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="parent2" name="ПІБ2" type="string" default="${lastName_UA3} ${firstName_UA3} ${middleName_UA3}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="visitDay" name="День та час зустрічі" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="workerDecision" name="Рішення працівника ДМС" type="enum" required="true">
          <activiti:value id="accept" name="Призначити зустріч у відділенні"></activiti:value>
          <activiti:value id="decline" name="Відмовити в наданні послуги"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decisionComment" name="Коментар до рішення; Наприклад, пояснення чому громадянину було відмовлено в наданні цієї послуги &lt;br /&gt; АБО &lt;br /&gt; Додаткові деталі для закінчення процессу реєстрації" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="taxAddress" name="Реквізити для сплати державного мита" type="textArea" default="Одержувач: УДКСУ у Бабушкінському районі м. Дніпропетровська, Банк УДКУ у Дніпропетровській обл., МФО 805012, Код ЄДРПОУ 37989274, Р/р 31416538700004, Код платежу 22090400"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression><![CDATA[${svid},${pasp1},${pasp2}]]></activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" name="${workerDecision=='decline'}" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='decline'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask5" name="E-Mail відмова в наданні послуги" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Оформлення паспорта громадянина України по досягненні 16-річного віку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
										На жаль, Вам відмовлено у надані послуги.
									&lt;/p&gt; 
									&lt;p&gt;
										Коментар Державного службовця:&lt;br /&gt;
											&lt;b&gt;&lt;i&gt;${decisionComment}&lt;/i&gt;&lt;/b&gt;
									&lt;/p&gt;
									&lt;p&gt;
										При вниникненні питань Ви можете звернутися до чергового:&lt;br /&gt;
										тел. +38 (050) 500-93-03
									&lt;/p&gt;
									[pattern/mail/_common_signature_start.html]
                                        Державна Міграційна Служба Дніпропетровської Області.&lt;/p&gt;
                                    [pattern/mail/_common_footer.html]
		  </activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" sourceRef="servicetask5" targetRef="endevent3"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow10" name="${workerDecision=='accept'}" sourceRef="exclusivegateway1" targetRef="servicetask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${workerDecision=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask6" name="E-Mail призначення візиту" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Оформлення паспорта громадянина України по досягненні 16-річного віку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
                                    &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
                                    &lt;p&gt;
                                        Спеціалісти відділу ДМС успішно опрацювли Вашу заяву.&lt;br /&gt;
										Для того щоб закінчити процедуру оформлення паспорта громадянина України по досягненні 16-річного віку необхідно:&lt;br /&gt;
										&lt;ul&gt;
										&lt;li&gt;У будь-якому банку сплатити Державне мито у розмірі 0.85 грн і отримати квитанцію про сплату.&lt;/li&gt;
										&lt;li&gt;Прийти до відділення ДМС за адресою: пр. Ілліча, 3 з необхідними документами та звернутись до каб. №4.&lt;/li&gt;
										&lt;li&gt;Підписати необхідні документи для видачі паспорту.&lt;/li&gt;
										&lt;/ul&gt;
									&lt;/p&gt;
									&lt;p&gt;
										&lt;b&gt;Реквізити для сплати:&lt;/b&gt;>&lt;br /&gt;
										&lt;i&gt;${taxAddress}&lt;/i&gt;
									&lt;/p&gt;
									&lt;p&gt;
										&lt;b&gt;Деталі зустрічі:&lt;/b&gt;&lt;br /&gt;
										День та час - ${visitDay}
									&lt;/p&gt;
									&lt;p&gt;
										&lt;b&gt;Необхідні документи, які треба принести до відділення ДМС при оформленні паспорта громадянина України по досягненні 16-річного віку:&lt;/b&gt;&lt;br/&gt;
										&lt;ul&gt;
										&lt;li&gt;свідоцтво про народження;&lt;/li&gt;
										&lt;li&gt;квитанцію про сплату державного мита або документ про звільнення від його сплати;&lt;/li&gt;
										&lt;li&gt;паспорт(и) батьків;&lt;/li&gt;
										&lt;li&gt;дві фотокартки розміром 3,5 х 4,5 см (фотокартки, що подаються для оформлення паспорта, мають бути виконані з одного негатива, із зображенням обличчя виключно анфас, без головного убору, виготовленими на тонкому білому або кольоровому фотопапері без кутика; для громадян, які постійно носять окуляри, обов'язкове фотографування в окулярах);&lt;/li&gt;
										&lt;/ul&gt;
									&lt;/p&gt;
									&lt;p&gt;
										Коментар Державного службовця:&lt;br /&gt;
										&lt;b&gt;&lt;i&gt;${decisionComment}&lt;/i&gt;&lt;/b&gt;
									&lt;/p&gt;
									&lt;p&gt;
										При вниникненні питань Ви можете звернутися до чергового:&lt;br /&gt;
										тел. +38 (050) 500-93-03
									&lt;/p&gt;
                                    [pattern/mail/_common_signature_start.html]
									Державна Міграційна Служба Дніпропетровської Області,&lt;br /&gt;
									[pattern/mail/_common_signature_end.html]   
									[pattern/mail/_common_footer.html]
		  </activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow27" sourceRef="servicetask6" targetRef="usertask2_red"></sequenceFlow>
    <userTask id="usertask2_red" name="Візит громадянина" activiti:candidateGroups="dnepr_dms-89_${district}">
      <extensionElements>
        <activiti:formProperty id="info1" name="На цьому етапі:" type="label" default="Громадянин приходить до Вас. Ви роздруковуєте заяву, підписуєте її та віддаєте начальнику підрозділу."></activiti:formProperty>
        <activiti:formProperty id="visitDay" name="День та час зустрічі" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clFio1" name="ПІБ заявника" type="string"  default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="ПІБ отримувача паспорта" type="string" default="${lastName_UA1} ${firstName_UA1} ${middleName_UA1}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsBirth" name="Дата народження отримувача паспорта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsPOB" name="Місце народження отримувача паспорта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="kidsSex" name="Стать" type="enum"  writable="false">
          <activiti:value id="man" name="Чоловіча"></activiti:value>
          <activiti:value id="woman" name="Жіноча"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="kids_liv" name="Місце проживання; Населений пункт, Вулиця, Будинок, Корпус, Квартира" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="lable2" type="label" default="Дані батьків дитини, яка буде отримувати паспорт"></activiti:formProperty>
        <activiti:formProperty id="parent1" name="ПІБ1" type="string" default="${lastName_UA2} ${firstName_UA2} ${middleName_UA2}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="parent2" name="ПІБ2" type="string" default="${lastName_UA3} ${firstName_UA3} ${middleName_UA3}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="district" name="Район м. Дніпропетровськ, до якого належить місце реєстрації громадянина" type="enum" writable="false">
          <activiti:value id="bab" name="Бабушкінський"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="result" name="Результат зустрічі" type="enum">
