        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення: Взяття на соціальний квартирний облік]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
