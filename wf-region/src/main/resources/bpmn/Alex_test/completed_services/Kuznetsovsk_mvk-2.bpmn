    <sequenceFlow id="flow14" sourceRef="startevent2" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="mailtask6" targetRef="taskRecieved"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
